{
  "name": "Gmail Dynamic Images",
  "version": "0.0.3",
  "manifest_version": 2,
  "description": "Update Gmail cached images with the real ones",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "permissions": [
    "tabs",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*"
      ],
      "js": [
        "js/jquery/jquery.min.js",
        "src/inject/inject.js"
      ]
    }
  ],
  "options_ui": {
    "page": "src/options.html",
    "chrome_style": true
  }
}